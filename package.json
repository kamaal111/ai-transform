{
  "name": "ai-transform",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "compile": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Kamaal Farah",
  "license": "MIT",
  "packageManager": "pnpm@10.9.0",
  "dependencies": {
    "@anthropic-ai/sdk": "~0.40",
    "@google/genai": "~0.10",
    "@oclif/core": "^4",
    "neverthrow": "^8",
    "openai": "^4",
    "zod": "^3"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@types/node": "^22",
    "@typescript-eslint/parser": "^8",
    "@vitest/coverage-v8": "^3",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-neverthrow": "^1",
    "globals": "^16",
    "husky": "^9",
    "lint-staged": "^15",
    "oclif": "^4",
    "prettier": "^3",
    "ts-node": "^10",
    "tsx": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vitest": "^3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "printWidth": 80,
    "tabWidth": 2
  },
  "bin": {
    "ai-transform": "./bin/run.js"
  },
  "oclif": {
    "bin": "ai-transform",
    "commands": "./dist/commands",
    "dirname": "ai-transform",
    "topicSeparator": " "
  }
}
